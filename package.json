  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts  build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "truffle:deploy": "truffle migrate --network ropsten --openssl-legacy-provider",
    "sol:lint": "./node_modules/.bin/solium -d ./contracts",
    "sol:test": "truffle test",
    "sol:coverage": "./node_modules/.bin/solidity-coverage",
    "ganache:start": "ganache-cli --network development",
    "truffle:migrate": "truffle migrate --network development --reset --compile-all",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "assert": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "https-browserify": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "os-browserify": "^0.3.0",
    "prettier": "^1.13.7",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "solidity-coverage": "^0.5.4",
    "solium": "^1.1.7",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "truffle": "^4.1.12",
    "truffle-assertions": "^0.3.1",
    "url": "^0.11.0"
  }
